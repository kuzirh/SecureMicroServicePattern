{
  "name": "src",
  "version": "0.1.0",
  "bin": {
    "src": "bin/src.js"
  },
  "scripts": {
    "build": "sh install.sh",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "prettier-check": "npx prettier -c .",
    "prettier-format": "npx prettier -w .",
    "eslint-check": "npx eslint . --ext .ts",
    "pre-commit": "npm run test && npm run prettier-check && npm run prettier-format && npm run eslint-check"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "aws-cdk": "2.41.0",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.41.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "testMatch": [
      "**/*.test.ts/**"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
